<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>MatchingScore__c</fullName>
    <externalId>false</externalId>
    <formula>0 +
IF(INCLUDES(MatchingReasons__c, &apos;Baufortschritt&apos;), 0.25, 0) +
IF(INCLUDES(MatchingReasons__c, &apos;Geografisch&apos;), 0.25, 0) +
IF(INCLUDES(MatchingReasons__c, &apos;Zweiter Bauabschnitt&apos;), 0.25, 0) +
IF(INCLUDES(MatchingReasons__c, &apos;Betreiber&apos;), 0.25, 0)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Matching Score</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackTrending>false</trackTrending>
    <type>Percent</type>
</CustomField>
