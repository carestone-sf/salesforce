<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>JahrDerAuszahlungOnlyNumbers</fullName>
    <active>true</active>
    <errorConditionFormula>NOT(REGEX(JahrDerAuszahlungText__c,&quot;^[0-9]*$&quot;))</errorConditionFormula>
    <errorDisplayField>JahrDerAuszahlung__c</errorDisplayField>
    <errorMessage>Das Jahr der Auszahlung darf keine Buchstaben enthalten.</errorMessage>
</ValidationRule>
