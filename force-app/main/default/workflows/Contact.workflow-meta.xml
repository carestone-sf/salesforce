    <rules>
        <fullName>Disable Skip Duplicate Check</fullName>
        <active>true</active>
        <criteriaItems>
            <field>Contact.Skip_Duplicate_Check__c</field>
            <operation>equals</operation>
            <value>True</value>
        </criteriaItems>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
        <workflowTimeTriggers>
            <actions>
                <name>Skip_Duplicate_Check_False</name>
                <type>FieldUpdate</type>
            </actions>
            <timeLength>1</timeLength>
            <workflowTimeTriggerUnit>Hours</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
    </rules>
    <rules>
        <fullName>E-Mail Maklerbetreuer setzen</fullName>
        <actions>
            <name>E_Mail_Maklerbetreuer_setzen</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <formula>AND(
NOT(ISBLANK(AccountId)),
OR(AND(
    NOT(ISNEW()),
    ISCHANGED(AccountId)
), ISNEW(),
 Daten_aktualisieren__c)
)</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Externe Maklerbetreuer E-Mail setzen Interessent</fullName>
        <actions>
            <name>Externe_Maklerbetreuer_E_Mail_setzen_Int</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <description>Für Kunden und Interessenten die Externe Maklerbetreuer E-Mail setzen</description>
        <formula>OR(
 AND(Daten_aktualisieren__c,
    OR(Includes(Gesch_ftsbeziehung__c, &apos;Kunde&apos;), Includes(Gesch_ftsbeziehung__c, &apos;Interessent&apos;))
),
AND(
    NOT(ISNEW()),
    ISCHANGED(AccountId),
    OR(Includes(Gesch_ftsbeziehung__c, &apos;Kunde&apos;), Includes(Gesch_ftsbeziehung__c, &apos;Interessent&apos;))
), AND(
ISNEW(),
 OR(Includes(Gesch_ftsbeziehung__c, &apos;Kunde&apos;), Includes(Gesch_ftsbeziehung__c, &apos;Interessent&apos;))))</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Externe Maklerbetreuer E-Mail setzen Makler</fullName>
        <actions>
            <name>Externe_Maklerbetreuer_E_Mail_setzen_Mak</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <description>Für Makler die Externe Maklerbetreuer E-Mail setzen</description>
        <formula>AND(
NOT(ISNULL(Account.ParentId)), 
OR(Includes(Gesch_ftsbeziehung__c, &apos;Versicherungsmakler&apos;), Includes(Gesch_ftsbeziehung__c, &apos;Immobilienmakler&apos;),
Includes(Gesch_ftsbeziehung__c, &apos;Immobilien- oder Versicherungsmakler&apos;),
Includes(Gesch_ftsbeziehung__c, &apos;Finanz- und Versicherungsmakler&apos;),
Includes(Gesch_ftsbeziehung__c, &apos;Finanzmakler&apos;)
),
OR(
Daten_aktualisieren__c,
AND(
    NOT(ISNEW()),
    ISCHANGED(AccountId)
),
ISNEW()
)
)</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
