        <criteriaItems>
            <field>Account.OverheadProvisionBis__c</field>
            <operation>notEqual</operation>
        </criteriaItems>
        <description>Wenn das Datum &quot;Overhead Provision bis&quot; erreicht wurde, sollte die Overhead Provision auf 0 gesetzt werden</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
        <workflowTimeTriggers>
            <actions>
                <name>Overhead_Provision_zur_cksetzen</name>
