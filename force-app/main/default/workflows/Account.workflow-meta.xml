            <actions>
                <name>Maklerschutz_l_uft_in_zwei_Wochen_aus</name>
                <type>Alert</type>
            </actions>
            <offsetFromField>Account.Maklerschutz_bis__c</offsetFromField>
            <timeLength>-14</timeLength>
            <workflowTimeTriggerUnit>Days</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
    </rules>
