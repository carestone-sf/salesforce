        <actions>
            <name>Maklerauftrag_beidseitig_unterschrieben</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>Account.CS_Vertriebsvereinbarung_Status__c</field>
            <operation>equals</operation>
            <value>Maklerauftrag beidseitig unterzeichnet</value>
        </criteriaItems>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
