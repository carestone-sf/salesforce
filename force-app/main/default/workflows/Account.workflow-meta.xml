    <rules>
        <fullName>CS Vertriebsvereinbarung Statusupdate</fullName>
        <actions>
            <name>CS_Vertriebsvereinbarung_Statusupdate</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <formula>ISCHANGED( CS_Vertriebsvereinbarung_Status__c  )</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
